<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bb"
        tools:context=".MainActivity">
    <TextView
            android:textSize="18dp"
            android:text="Time"
            android:layout_width="79dp"
            android:layout_height="30dp"
            android:id="@+id/textView8" android:layout_marginTop="280dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/editText4"/>
    <TextView
            android:textSize="18dp"
            android:text="Date"
            android:layout_width="79dp"
            android:layout_height="30dp"
            android:id="@+id/textView11" android:layout_marginTop="200dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/editText3"/>
    <TextView
            android:textSize="18dp"
            android:text="Appt Details"
            android:layout_width="77dp"
            android:layout_height="43dp"
            android:id="@+id/textView10" android:layout_marginTop="120dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/editText2"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText" android:layout_marginTop="30dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/textView12"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            android:text="XXXXXXXXXX"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText2"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="120dp"
            app:layout_constraintStart_toEndOf="@+id/textView10" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:text="XXXXXXXXXX"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText3" android:layout_marginTop="200dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/textView11"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            android:text="XXXXXXXXXX"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText4" android:layout_marginTop="280dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            android:text="XXXXXXXXXX"/>
    <TextView
            android:textSize="18dp"
            android:text="Appt Id"
            android:layout_width="81dp"
            android:layout_height="42dp"
            android:id="@+id/textView12" android:layout_marginTop="30dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/editText"/>
    <Button
            android:textSize="15dp"
            android:text="Delete"
            android:layout_width="161dp"
            android:layout_height="66dp"
            android:id="@+id/button" android:layout_marginTop="450dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toEndOf="@+id/button2"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            android:background="@drawable/round"/>
     <Button
            android:textSize="15dp"
            android:text="CREATE"
            android:layout_width="161dp"
            android:layout_height="66dp"
            android:id="@+id/button2" android:layout_marginTop="448dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button"
            android:background="@drawable/round"/>
</androidx.constraintlayout.widget.ConstraintLayout>